struct Integers {
  byte: u8;
  uword: u16;
  ulong: u32;
  sword: i16;
  slong: i32;
}

struct String {
  string: str;
}

struct ArrayIntegers {
  count: u8;
  values: [u16;count];
}

struct ArrayStructs {
  count: u16;
  values: [ArrayIntegers;count];
}

struct ArrayIntegersNullTerminated {
  values: [u16;null];
}

struct SimpleStruct {
  first: u16;
  last: u32;
}

struct ArrayStructsNullTerminated {
  values: [SimpleStruct;null];
}

struct LessThanOrEqual {
  count: u16;
  values: [u16;<=count];
}

struct ByteWidth {
  twotofour: u32#2;
  threetofour: u32#3;
  byte: u8;
  last: u32#3;
}
